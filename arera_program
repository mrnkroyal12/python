l=eval(input("enter the len"))
b=eval(input("enter the breadth"))
print("\farea of rect",l*b)
r=eval(input("enter the raadius"))
print("area of circle",3.14*r*r)
a=eval(input("enter the side"))
print("area of sqaure",a*a)

